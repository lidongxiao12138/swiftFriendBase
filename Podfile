target 'SwiftProduct' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for SwiftProduct
  pod 'RxCocoa'                     #RX组件
  
  pod 'Moya/RxSwift', '~> 10.0.0'    #网络请求 (swift)
  
  pod 'ObjectMapper', '~> 3.0.0'     #数据解析 (swift)
  
  pod 'Kingfisher'                  #图片加载 (swift)
  
  pod 'MJRefresh'                   #刷新 (oc)
  
  pod 'SnapKit'                     #布局约束 (swift)
  
  pod 'CYLTabBarController'          #拓展的Tabbar (oc)
  
  pod 'DZNEmptyDataSet'              #空视图
  
  pod 'IQKeyboardManager'            #键盘管理 (oc)
  
  pod 'SwiftTheme'                  #第三方主题库(Swift)
end
